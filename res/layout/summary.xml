<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/laytop"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true"
    android:background="@drawable/bg"
	android:padding="5dp"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	
    <RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		<LinearLayout
			android:layout_width="wrap_content"
			android:layout_alignParentLeft="true"
			android:layout_centerVertical="true"
			android:layout_height="wrap_content">
			<LinearLayout
				android:layout_width="20dp"
				android:layout_height="match_parent"
				android:background="#CC00CC"/>
			<TextView
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:textColor="#000000"
		        android:paddingLeft="2dp"
		        android:text="@string/resulttitle"
		        android:textSize="20sp" />
		</LinearLayout>
		<Button
		    android:id="@+id/cmdOverall"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="#CC00CC"
		    android:textColor="#ffffff"
		    android:layout_centerVertical="true"
		    android:padding="10dp"
		    android:layout_alignParentRight="true"
		    android:textSize="16sp"
		    android:text="@string/overall" />
	</RelativeLayout>
	
    <RelativeLayout
        android:id="@+id/layngf"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginTop="10dp"
		android:background="@drawable/expmainbg">
	    
		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/txtNGFResult"
			android:text="@string/empty"
			android:textSize="16sp"
			android:layout_centerVertical="true"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"
			android:layout_marginTop="5dp"
			android:layout_toLeftOf="@+id/imgNGF"
			/>
		
		<ImageButton
			android:id="@+id/imgNGF"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:contentDescription="@string/empty"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			android:clickable="true"
			android:background="@android:color/transparent"
			android:src="@drawable/i_letter_selector"
			/>
	</RelativeLayout>
	
    <RelativeLayout
        android:id="@+id/layfsh"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginTop="10dp"
		android:background="@drawable/expmainbg">
	    
		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/txtFshResult"
			android:text="@string/empty"
			android:textSize="16sp"
			android:layout_centerVertical="true"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"
			android:layout_marginTop="5dp"
			android:layout_toLeftOf="@+id/imgFSH"
			/>
		
		<ImageButton
			android:id="@+id/imgFSH"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:contentDescription="@string/empty"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			android:clickable="true"
			android:background="@android:color/transparent"
			android:src="@drawable/i_letter_selector"
			/>

	</RelativeLayout>
	
    <RelativeLayout
        android:id="@+id/laymma"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginTop="10dp"
		android:layout_marginBottom="10dp"
		android:background="@drawable/expmainbg">
	    
		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/txtMMAResult"
			android:text="@string/empty"
			android:textSize="16sp"
			android:layout_centerVertical="true"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"
			android:layout_marginTop="5dp"
			android:layout_toLeftOf="@+id/imgMMA"
			/>
		
		<ImageButton
			android:id="@+id/imgMMA"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:contentDescription="@string/empty"
			android:layout_marginTop="5dp"
			android:layout_marginRight="5dp"
			android:layout_centerVertical="true"
			android:layout_alignParentRight="true"
			android:clickable="true"
			android:background="@android:color/transparent"
			android:src="@drawable/i_letter_selector"
			/>

	</RelativeLayout>
	
    <com.cs.pausis.ActionSlideExpandableListView
		android:id="@+id/list"
		android:layout_height="fill_parent"
		android:layout_width="fill_parent"
		android:layout_margin="5dp"
		android:divider="@android:color/transparent"
	  	android:dividerHeight="10.0sp"/>
</LinearLayout>